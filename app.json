{
  "expo": {
    "name": "shinmini-animation",
    "slug": "shinmini-animation",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/app_config/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/app_config/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/app_config/car-sample.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.shinmini.horizontalanimation"
    },
    "web": {
      "favicon": "./src/assets/app_config/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d5b7b9a4-24ea-4ec2-86e2-64c19ba61495"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}
