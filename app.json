{
  "expo": {
    "name": "shinmini-app",
    "slug": "shinmini-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_config/icon.png",
    "scheme": "miniapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/app_config/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hyeonmini.shinminiapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app_config/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.hyeonmini.shinminiapp"
    },
    "web": {
      "favicon": "./assets/images/app_config/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "88b75fa1-e951-4ff1-988a-3f03fb2018eb"
      }
    }
  }
}
